# Docker
DOCKER_TLS_VERIFY=1
DOCKER_CERT_PATH="~/Documents/certs/docker"
DOCKER_HOST="tcp://<IP>:2376"

# zsh theme p10k
POWERLEVEL9K_TRANSIENT_PROMPT=same-dir
POWERLEVEL10K_TRANSIENT_PROMPT=same-dir

# psql
PGUSER=postgres
PGPASSWORD=postgres
PGHOST=localhost
PGPORT=5432
PGDATABASE=postgres

# Dynamic DNS Configuration
# Set your custom DNS server address (default: 8.8.8.8 if not set)
DNS_SERVER_ADDRESS="<YOUR_DNS_SERVER_IP>"

# LPU Captive Portal Configuration
# Portal URLs and server settings
LPU_PORTAL_URL="https://internet.lpu.in/24online/servlet/E24onlineHTTPClient"
LPU_PORTAL_REFERER="https://internet.lpu.in/24online/webpages/client.jsp"
LPU_PORTAL_ORIGIN="https://internet.lpu.in"
LPU_SERVER="<LPU_INTERNAL_SERVER_IP>"

# WiFi Network Names for Dynamic DNS
# Exact match WiFi names (comma-separated)
# WIFI_NAMES_EXACT="NetworkName1,NetworkName2"
# Substring match WiFi names (comma-separated)
# WIFI_NAMES_SUBSTRING="Partial1,Partial2"

# LPU WiFi Network Names for Auto-Login
# Exact match LPU WiFi names (comma-separated)
LPU_WIFI_NAMES_EXACT="LPU"
# Substring match LPU WiFi names (comma-separated)
LPU_WIFI_NAMES_SUBSTRING="LPU,lpu"
